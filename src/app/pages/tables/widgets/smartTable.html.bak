<div class="horizontal-scroll">
  <div class="form-group select-page-size-wrap ">
    <label>Rows on page
      <select class="form-control selectpicker show-tick" title="Rows on page" selectpicker
              ng-model="smartTablePageSize" ng-options="i for i in [5,10,15,20,25]">
      </select>
    </label>
  </div>
  <table class="table" st-table="smartTableDataN">
    <thead>
    <tr class="sortable ">
      <th class="table-id" st-sort="job_id" st-sort-default="true">job_id</th>
      <th st-sort="job">job</th>
      <th st-sort="queue">queue</th>
      <th st-sort="user">user</th>
      <th st-sort="status">status</th>
    </tr>
    <tr>
      <th><input st-search="job_id" placeholder="Search Job Id" class="input-sm form-control search-input" type="search"/></th>
      <th><input st-search="job" placeholder="Search Job Name" class="input-sm form-control search-input" type="search"/></th>
      <th><input st-search="queue" placeholder="Search Queue" class="input-sm form-control search-input" type="search"/></th>
      <th><input st-search="user" placeholder="Search User" class="input-sm form-control search-input" type="search"/></th>
      <th><input st-search="status" placeholder="Search Status" class="input-sm form-control search-input" type="search"/>
      </th>
    </tr>
    </thead>
    <tbody>
      <!--
        <tr ng-repeat="item in smartTableDataN">
          <td class="table-id">{{item.id}}</td>
          <td>{{item.firstName}}</td>
          <td>{{item.lastName}}</td>
          <td>{{item.username}}</td>
          <td><a class="email-link" ng-href="mailto:{{item.email}}">{{item.email}}</a></td>
          <td>{{item.age}}</td>
        </tr>
      -->
      <tr ng-repeat="item in smartTableDataN">
        <td class="table-id">{{item.job_id}}</td>
        <td>{{item.job}}</td>
        <td>{{item.queue}}</td>
        <td>{{item.user}}</td>
        <td>{{item.status}}</td>
      </tr>
    </tbody>
    <tfoot>
    <tr>
      <td colspan="6" class="text-center">
        <div st-pagination="" st-items-by-page="smartTablePageSize" st-displayed-pages="5"></div>
      </td>
    </tr>
    </tfoot>
  </table>
</div>
